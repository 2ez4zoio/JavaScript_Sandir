

lista = [8, 3]

function troca(lista, pos1, pos2){

    let aux = lista[pos1];
    console.log("1 - lista("+lista+")"+"auxiliar: "+aux);

    lista[pos1] = lista[pos2];
    console.log("2 - lista("+lista+")"+"auxiliar: "+aux)

    lista[pos2] = aux;
    console.log("3 - lista("+lista+")"+"auxiliar: "+aux);

    return lista;

}

var lista = [8, 3]
troca(lista, 0 ,1)
console.log(lista)
