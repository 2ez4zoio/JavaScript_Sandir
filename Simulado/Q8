<!DOCTYPE html> //08
<head>
    <meta charset="UTF-8">
</head>
<body>

    <h1>Maior Valor</h1>

    <script>
        var numeros = [10, 20, 30, 40, 50, 60, 70, 80, 100];

        var maiorValor = numeros[0];

        for (var i = 1; i < numeros.length; i++) {
            if (numeros[i] > maiorValor) {
                maiorValor = numeros[i];
            }
        }
        document.write("<p>Array: [" + numeros.join(", ") + "]</p>");
        document.write("<p>Maior valor: " + maiorValor + "</p>");
    </script>

</body>
</html>
