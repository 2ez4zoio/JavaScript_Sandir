<body>

    <h1>Array Invertido</h1>

    <script>
        var elementos = [1, 2, 3];
        function inverterArray(arr) {
            return arr.reverse();
        }

        document.write("<p>Array padr√£o: [" + elementos.join(", ") + "]</p>");
        document.write("<p>Array invertido: [" + inverterArray(elementos).join(", ") + "]</p>");
    </script>

</body>
